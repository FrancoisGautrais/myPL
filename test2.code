def eratostene(n) {
    firsts=list()
    firsts.append(1)
    all=list()
    for(i=2; i<n; i++) all.append(i)

    while(all.length()>0){
        x=all.at(0)
        all.remove(x)
        firsts.append(x)

        for(i=all.length()-1; i>=0; i--){

            if(all.at(i)%x==0){
                all.remove(all[i])
            }
        }
    }
    return firsts
}

def decompose(n){
    out=list()
    premiers=eratostene(int(n/2)+1)
    for(i=premiers.length()-1; i>=0; i--){
        for(j=premiers.length()-1; j>=0; j--){
            a=premiers.at(i)
            b=premiers.at(j)
            if(a*b<=n){
                tmp=list()
                tmp.append(a)
                tmp.append(b)
                out.append(tmp)
                n-=a*b
                if(n==0) return out

            }
        }
    }
    print("End")
    return out
}

def assemble(arr)
{
    print(arr)
    out=""
    acc=0
    for(i=0; i<arr.length(); i++)
    {
        if(i>0) out+="+"
        x=arr.at(i)
        out+=string(x.at(0))+"*"+string(x.at(1))
        acc+=x.at(0)*x.at(1)
    }
    return out+"="+string(acc)
}

t=timestamp()
x=decompose(1310)
assemble(x)
print("in ", timestamp()-t, " s")
